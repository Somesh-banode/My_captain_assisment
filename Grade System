# include <conio.h>
# include <stdio.h>
 main()
{
	printf("enter your Marks obtain out of 100 = ");
	int marks=0;
	scanf("%d",&marks);
	if (marks>=85)
	printf("your Grade is A ");
	else 
		if (marks>=70)
		printf("your Grade is B ");
		else 
			if (marks>=55)
		    printf("your Grade is C ");
			else 
			    if (marks>=40)
				printf("your Grade is D ");
			    else 
				printf("your Grade is F ");
				
}
